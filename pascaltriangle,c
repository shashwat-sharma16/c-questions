#include<stdio.h>
int ncr(int n, int r){
    int facn=1,facr=1,facnr=1,i;
    for(i=1;i<=n;i++){
        facn=facn*i;
    }
     for(i=1;i<=r;i++){
        facr=facr*i;
    }
     for(i=1;i<=n-r;i++){
        facnr=facnr*i;
    }
    int result=facn/(facr*facnr);
    return result;
}
int main(){
    printf("enter n:\n");
    int n;
    scanf("%d", &n);
    int i,j;
    for(i=0;i<=n;i++){
        for(j=0;j<=i;j++){
             printf("%d", ncr(i,j));
        }
        printf("\n");
    }
    return 0;
}
